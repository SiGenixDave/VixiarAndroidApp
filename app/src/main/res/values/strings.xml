    <!-- Messages -->
    <string name="dlg_title_ble_not_supported">BLE NOT SUPPORTED</string>
    <string name="dlg_msg_ble_not_supported">This device does not support Bluetooth Low Energy (BLE)
        which is required to communicate to the handheld.</string>

    <string name="dlg_title_ble_enable_pre">BLE MUST BE ENABLED</string>
    <string name="dlg_msg_ble_enable_pre">The next screen will allow you to enable Bluetooth
        Low Energy (BLE).  Please enable BLE so this app can communicate
        with handheld devices.</string>

    <string name="dlg_title_ble_not_enabled">BLE MUST BE ENABLED</string>
    <string name="dlg_msg_ble_not_enabled">Bluetooth Low Energy (BLE) must be enabled.
        BLE is required to communicate with the Vixiar handheld device.</string>

    <string name="dlg_title_location_service">LOCATION ACCESS</string>
    <string name="dlg_msg_location_service">Android must have location services
        enabled in order to connect to the Vixiar handheld device. This is a requirement
        from Google for any app connecting to a Bluetooth Low Energy (BLE) device. To enable
        location services, open your devices settings app, scroll to location and tap.  Then turn on
        location services.
        Restart this app once location services are enabled.</string>

    <string name="dlg_title_location_access_pre">LOCATION ACCESS REQUIRED</string>
    <string name="dlg_msg_location_access_pre">The next screen will allow you to grant location
        access to this app.  Please grant location access so this app can communicate
        with handheld devices.</string>

    <string name="dlg_title_location_access_no">LOCATION ACCESS REQUIRED</string>
    <string name="dlg_msg_location_access_no">Since location access has not been granted, this app
        will not be able to communicate with handheld devices.</string>

    <string name="dlg_title_authentication_error">PAIRING ISSUE</string>
    <string name="dlg_msg_authentication_error">A handheld device was detected, however it is
        not paired with this tablet. It may show as paired in the Android settings, but could have since
        been paired with a different tablet. See Instructions for Use for instructions on how to
        pair the handheld with this device.</string>

    <string name="dlg_title_no_paired_devices">NO PAIRED DEVICES</string>
    <string name="dlg_msg_no_paired_devices">A handheld device was detected, however it is
        not paired with this tablet. See Instructions for Use for instructions on how to
        pair the handheld with this device.</string>

    <string name="dlg_title_no_handhelds">NO HANDHELD FOUND</string>
    <string name="dlg_msg_no_handhelds">No Indicor handhelds were detected.  Please make sure
        the Indicor handheld is powered on and the blue LED is flashing.</string>

    <string name="dlg_title_pressure_error_start">ERROR</string>
    <string name="dlg_msg_pressure_error_start">You have 10 seconds from when the test starts
        to hold the pressure in the testing range.</string>

    <string name="dlg_title_pressure_error_running">ERROR</string>
    <string name="dlg_msg_pressure_error_running">You must maintain the pressure within the testing
        range until the test is complete.</string>

    <string name="dlg_title_cancel_test">CANCEL TEST?</string>
    <string name="dlg_msg_cancel_test">Do you want to cancel this test.  If you do, patient
        information will not be saved.</string>

    <string name="dlg_title_hr_not_stable">HEART RATE NOT STABLE</string>
    <string name="dlg_msg_hr_not_stable">The heart rate did not become stable within the allotted
        time period.</string>

    <string name="dlg_title_connection_problem">PROBLEM CONNECTING</string>
    <string name="dlg_msg_connection_problem">Theere was a problem connecting to the handheld device.
        Try restarting it and connecting again.</string>

    <string name="dlg_title_storage_access_pre">STORAGE ACCESS REQUIRED</string>
    <string name="dlg_msg_storage_access_pre">The next screen will allow you to enable writing to
        files.  Please enable "Storage" so this app can save data files.</string>

    <string name="dlg_title_storage_access_no">STORAGE ACCESS REQUIRED</string>
    <string name="dlg_msg_storage_access_no">Since storage access has not been granted, this app
        will not be able to communicate with handheld devices.</string>

    <string name="dlg_title_handheld_timeout">LOST CONNECTION</string>
    <string name="dlg_msg_handheld_timeout">It appears that the connection to the handheld device
        has been lost.  Make sure the power remains on and the device stays within range of the
        tablet.</string>

    <string name="dlg_title_sequene_error">COMMUNICATIONS ERROR</string>
    <string name="dlg_msg_sequence_error">There was an error in the data received from the handheld.
        The sequence numbers were not correct.</string>
